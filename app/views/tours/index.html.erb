<h1>Tours</h1>

<table>
  <thead>
    <tr>
      <th>Titre</th>
      <th colspan="3">Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @tours.each do |tour| %>
      <tr>
        <td><%= tour.title %></td>
        <td><%= link_to 'Show', tour %></td>
        </tr>
    <% end %>
  </tbody>
</table>
