<!DOCTYPE html>
<html>
  <head>
    <title>BraaamTour</title>
    <%= csrf_meta_tags %>
    <meta content="width=device-width, initial-scale=1" name="viewport">
    <%= stylesheet_pack_tag "application" %>
  </head>

  <body class="body">
    <div class="flex">
      <%= render 'admin/nav' %>

      <p class="notice"><%= notice %></p>
      <p class="alert"><%= alert %></p>
      <%= yield %>
    </div>

    <%= javascript_pack_tag "application" %>
  </body>
</html>
