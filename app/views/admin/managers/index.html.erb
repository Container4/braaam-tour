<p id="notice"><%= notice %></p>

<h1>Admin Managers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_managers.each do |admin_manager| %>
      <tr>
        <td><%= link_to 'Show', admin_manager_path(admin_manager) %></td>
        <td><%= link_to 'Edit', edit_admin_manager_path(admin_manager) %></td>
        <td><%= link_to 'Destroy', admin_manager_path(admin_manager),
                                    method: :delete,
                                    data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Manager', new_admin_manager_path %>
